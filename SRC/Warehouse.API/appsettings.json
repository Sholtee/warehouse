{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "Endpoints": {
            "HttpsInlineCertAndKeyFile": {
                "Url": "https://localhost:1986",
                "Certificate": {
                    "Path": "Cert/certificate.crt",
                    "KeyPath": "Cert/private.key"
                }
            }
        }
    }
}
