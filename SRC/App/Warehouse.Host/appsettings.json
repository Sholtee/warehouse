{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {Message:l}{NewLine}{Properties:j}{NewLine}"
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "Auth": {
        "SessionExpirationMinutes": 1440,
        "SessionCookieName": "warehouse-session"
    },
    "Swagger": {
        "Version": "v1",
        "Title": "Warehouse API",
        "Description": "Warehouse management API",
        "Contact": {
            "Name": "Denes Solti",
            "Email": "sodnaatx@gmail.com"
        }
    },
    "RateLimiting": {
        // maximum 100 requests in each minutes
        "Fixed": {
            "Window": "00:01:00",
            "PermitLimit": 100
        }
    }
}